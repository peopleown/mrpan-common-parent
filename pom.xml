    </modules>
    <profiles>
        <!-- Dev -->
        <profile>
            <id>dev</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <build>
                <filters>
                    <filter>../mrpan-common-config/src/main/resources/variable-dev.properties</filter>
                </filters>
                <resources>
                    <resource>
                        <filtering>true</filtering>
                        <directory>../mrpan-common-config/src/main/resources/config</directory>
                        <!-- <targetPath>../shby-common-config/src/main/resources</targetPath> -->
                        <includes>
                            <include>config/*.properties</include>
                            <include>config/*.xml</include>
                            <include>config/*.conf</include>
                        </includes>
                    </resource>
                </resources>
            </build>
        </profile>
        <!-- 生产环境 -->
        <profile>
            <id>product</id>
            <activation>
                <activeByDefault>false</activeByDefault>
            </activation>
            <build>
                <filters>
                    <filter>../mrpan-common-config/src/main/resources/variable-product.properties</filter>
                </filters>
                <resources>
                    <resource>
                        <filtering>true</filtering>
                        <directory>../mrpan-common-config/src/main/resources/config</directory>
                        <!-- <targetPath>../shby-common-config/src/main/resources/config/</targetPath> -->
                        <includes>
                            <include>config/*.properties</include>
                            <include>config/*.xml</include>
                            <include>config/*.conf</include>
                        </includes>
                    </resource>
                </resources>
            </build>
        </profile>
        <!-- 测试环境 -->
        <profile>
            <id>test</id>
            <activation>
                <activeByDefault>false</activeByDefault>
            </activation>
            <build>
                <filters>
                    <filter>../mrpan-common-config/src/main/resources/variable-test.properties</filter>
                </filters>
                <resources>
                    <resource>
                        <filtering>true</filtering>
                        <directory>../mrpan-common-config/src/main/resources/config</directory>
                        <includes>
                            <include>config/*.properties</include>
                            <include>config/*.xml</include>
                            <include>config/*.conf</include>
                        </includes>
                    </resource>
                </resources>
            </build>
        </profile>
    </profiles>
